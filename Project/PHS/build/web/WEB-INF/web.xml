<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Comments</servlet-name>
        <servlet-class>PHSServlets.Comments</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestReport</servlet-name>
        <servlet-class>PHSServlets.TestReportProcess</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestsToDisplay</servlet-name>
        <servlet-class>PHSServlets.TestsToDisplay</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailReport</servlet-name>
        <servlet-class>PHSServlets.DetailReport</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MedicineIssued</servlet-name>
        <servlet-class>PHSServlets.MedicineIssued</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MedicineIssueSucces</servlet-name>
        <servlet-class>PHSServlets.MedicineIssueSucces</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Comments</servlet-name>
        <url-pattern>/Comments</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestReport</servlet-name>
        <url-pattern>/TestReport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestsToDisplay</servlet-name>
        <url-pattern>/TestsToDisplay</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailReport</servlet-name>
        <url-pattern>/DetailReport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MedicineIssued</servlet-name>
        <url-pattern>/MedicineIssued</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MedicineIssueSucces</servlet-name>
        <url-pattern>/MedicineIssueSucces</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file> MedicinesToIssue.jsp</welcome-file>
    </welcome-file-list>
</web-app>
